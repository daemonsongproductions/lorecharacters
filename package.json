{
  "name": "lorecharacters",
  "private": true,
  "dependencies": {
    "@rails/webpacker": "^3.5.5",
    "axios": "^0.18.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "bootstrap": "^4.1.3",
    "classnames": "^2.2.6",
    "coffeescript": "1.12.7",
    "jest": "^23.6.0",
    "moment": "^2.22.2",
    "moxios": "^0.4.0",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-redux": "^5.1.1",
    "react-test-renderer": "^16.6.3",
    "react_ujs": "^2.4.4",
    "reactstrap": "^6.5.0",
    "redux": "^4.0.1",
    "redux-mock-store": "^1.5.3",
    "redux-thunk": "^2.3.0",
    "webpack-cli": "^3.1.2",
    "webpacker-react": "^0.3.2"
  },
  "devDependencies": {
    "webpack-dev-server": "^2.11.3"
  },
  "jest": {
    "testMatch": [
      "**/__tests__/**/*.js?(x)"
    ],
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!react-native|react-navigation)"
    ],
    "collectCoverage": true,
    "setupTestFrameworkScriptFile": "<rootDir>/app/javascript/setupTests.js",
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/app/javascript/__mocks__/fileMock.js",
      "\\.(css|scss|sass)$": "<rootDir>/app/javascript/__mocks__/styleMock.js"
    }
  }
}
